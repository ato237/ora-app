{
  "expo": {
    "name": "Orramo",
    "slug": "Orramo-apk",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#14213D",
      "userInterfaceStyle": "automatic"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.atoproduction.orramo",
      "buildNumber": "1.0.9",
      "supportsTablet": true,
      "userInterfaceStyle": "light",
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-7148038859151468~4633361491"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses localization to check the users country and device language in order to give the proper available translations in-app.",
        "NSUserTrackingUsageDescription": "This app uses tracking inorder to set the language either authomatically or manually and to serve ads",
        "NSContactsUsageDescription ":"Allow Orramo to Access your contacts"
      }
      
    },
    "android": {
      "versionCode": 9,
      "package": "com.atoproduction.orramo",
      "userInterfaceStyle": "light",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#fff"
      },
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-7148038859151468~4452246712"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
    
      ]
    ]
  }
}
